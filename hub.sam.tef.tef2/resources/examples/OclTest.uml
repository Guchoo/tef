<?xml version="1.0" encoding="UTF-8"?>
<ocl.uml:Constraint xmi:version="2.1" xmlns:xmi="http://schema.omg.org/spec/XMI/2.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" xmlns:ocl.expr="http://www.eclipse.org/OCL2/1.0.0/ocl/expressions" xmlns:ocl.uml="http://www.eclipse.org/OCL2/1.0.0/ocl/uml" xsi:schemaLocation="http://www.eclipse.org/OCL2/1.0.0/ocl/expressions http://www.eclipse.org/OCL2/1.0.0/ocl#//expressions http://www.eclipse.org/OCL2/1.0.0/ocl/uml http://www.eclipse.org/OCL2/1.0.0/ocl#//uml">
  <body xsi:type="ocl.expr:LetExp">
    <in xsi:type="ocl.expr:IteratorExp" name="collect">
      <source xsi:type="ocl.expr:PropertyCallExp">
        <source xsi:type="ocl.expr:VariableExp" referredVariable="//@body/self"/>
        <referredProperty xsi:type="ecore:EReference" href="platform:/resource/Test/test.ecore#/1/AClass/testRef"/>
      </source>
      <body xsi:type="ocl.expr:PropertyCallExp">
        <source xsi:type="ocl.expr:VariableExp" referredVariable="//@body/collect/a"/>
        <referredProperty xsi:type="ecore:EAttribute" href="platform:/resource/Test/test.ecore#/1/AClass/name"/>
      </body>
      <iterator name="a"/>
    </in>
    <variable name="self">
      <type xsi:type="ecore:EClass" href="platform:/resource/Test/test.ecore#/1/AClass"/>
    </variable>
  </body>
</ocl.uml:Constraint>
